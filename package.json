{
  "name": "@guebbit/js-toolkit",
  "version": "1.0.5",
  "license": "AGPLv3.0",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "README.md",
    "LICENSE",
    "CHANGELOG"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:target": "jest /tests/getColorBrightness.spec.ts",
    "lint": "eslint \"*/**/*.{ts,js,json}\"",
    "lint:fix": "eslint \"*/**/*.{ts,js,json}\" --fix",
    "docs:dev": "vitepress dev docs --port 8080",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublish": "npm run build && npm run test && npm run lint && npm run docs:build",
    "publish:public": "npm run prepublish && npm publish --access public"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitepress": "^1.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prepublish"
    }
  }
}
